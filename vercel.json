{
  "version": 2,
  "builds": [
    {
      "src": "speedvoice_backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "speedvoice_backend/wsgi.py"
    },
    {
      "src": "/*",
      "dest": "speedvoice_backend/wsgi.py"
    }
  ]
}